$sidebarBackgroundColor: #343434;
$sidebarBackgroundColor: #C5C5C5;
$sidebarWidth: 200px;
$headerHeight: 50px;
$aPaddingTop: 18px;
$imgDim: 32px;

body {
  margin: 0;
  font-family: "Lato-Regular", "Lucida Grande";
  font-size: 14px;
  background-color: #e3e3e3;
}

#header {
  z-index: 1000;
  @include box-shadow(#000000 0px 0px 12px);
  height: $headerHeight;
  width: 100%;

  @include background-image(linear-gradient(#990000, #641B1B));
  font-family: "Sansation";

  position: fixed;
  left: 0;
  top: 0;

  #logo {
    font-family: "Freshman";
    font-size: 33px;
    position: absolute;
    height: $headerHeight - 20px;
    width: $sidebarWidth + 2px;
    padding: 10px 0px;
    background-color: transparentize(#000000, 0.8);

    a {
      text-align: center;
      display: block;
      text-decoration: none;
      color: white;
      @include text-shadow(#000000 1px 1px 4px);
    }
  }

  #links {
    position: absolute;
    right: 30px;
    font-size: 120%;
    z-index: 100;

    .menu li {
      display: inline-block;
      margin-right: 5px;
      vertical-align: top;
      width: 150px;

      a {
        height: $headerHeight - $aPaddingTop;
        display: block;
        background: {
          size: $imgDim $imgDim;
          repeat: no-repeat;
          position: 15px center;
        };
        padding: $aPaddingTop 20px 0 $imgDim+25px;
        color: white;
        text-decoration: none;


      }

      &.events .menuitem {
        background-image: image-url("common/header/calendar.png");
      }
      &.people .menuitem {
        background-image: image-url("common/header/people.png");
      }
      &.events.hover .menuitem, &.people.hover .menuitem {
        background-color: transparentize(#000000, 0.8);
      }

      &.wiki {
        .menuitem {
          background-image: image-url("common/header/wiki.png");
        }

        .staff {
          background-image:
            image-url("common/header/wiki/staff.png");
        }    
        .confi {
          background-image:
            image-url("common/header/wiki/confi.png");
        }    
        .basecamp {
          background-image:
            image-url("common/header/wiki/basecamp.png");
        }    

        .hover {
          .staff {
            background-image:
              image-url("common/header/wiki/staff_hover.png");
          }    
          .confi {
            background-image:
              image-url("common/header/wiki/confi_hover.png");
          }    
        }
      }

      .submenu {
        display:none;

        @include background-image(linear-gradient(#333333, #000000));
        @include border-bottom-radius(10px);
        @include single-box-shadow;

        font-size: 90%;
        margin-bottom: 10px;
        padding-bottom: 10px;

        li {
          display: list-item;

          a {
            margin: 0;
            padding: 14px 0 5px (16px+8px+25px);

            color: white;
            height: 20px;

            background: {
              size: 16px;
              repeat: no-repeat;
              position: 15px center;
            };

            &.event_list {
              background-image:
                image-url("common/header/event/event_list.png");
            }
            &.event_create {
              background-image:
                image-url("common/header/event/event_create.png");
            }
            &.calendar {
              background-image:
                image-url("common/header/event/calendar.png");
            }
          }

          &.hover {
            @include background-image(
              linear-gradient(#cccccc, #777777)
            );
            background-size: 100% 100%;
            background-position: 0 0;

            a {
              color: black;

              &.event_list {
                background-image:
                  image-url("common/header/event/event_list_hover.png");
              }
              &.event_create {
                background-image:
                  image-url("common/header/event/event_create_hover.png");
              }
              &.calendar {
                background-image:
                  image-url("common/header/event/calendar_hover.png");
              }
            } 
          }
        }
      }

    }
  }
}

#sidebar {
  $marginButton: 3%;

  height: 100%;
  width: $sidebarWidth;
  position: fixed;
  left: 0;
  top: $headerHeight;

  font-size: 110%;

  background: {
    color: $sidebarBackgroundColor;
  };
  border-right: {
    style: solid;
    color: #B3B3B3;
    width: 2px;
  };

  .sidebar-box-wrapper {
    .label {
      font:{
        size: 100%;
        family: "Lato-Black";
      };
      margin: 15px 0 10px 10px;
    }
    margin: 20px 0;
  }

  .sidebar-box, #user_search, #search {
    margin: 5px 5px 10px 5px;
    padding: 5px;
    width: $sidebarWidth - 10px - 10px - 5px;
    font-size: 100%;
  }

  .sidebar-box {
    background: transparentize(#000000, 0.9);
    border: {
      style: solid;
      color: transparentize(#000000, 0.8);
      width: 1px;
    };
    @include border-radius(5px);

    .sort_by {
      .ui-button-text {
        text-align: left;
        padding-left: 20px;
      }

    }
  }

  #user_search, #search {
    height: 32px;
    width: 94%;
    margin-top: 15px;
  }

  #checkboxes_house, #checkboxes_floor {
    label {
      margin: 5px;
    }
  }

  #navigation {
    $navItemFontSize: 115%;

    li {
      a {
        font: {
          family: "Lato-Bold";
          size: $navItemFontSize;
        };
        text-align: left;
        width: 92%;
        padding: 2%;
        margin: 2%;
      }
    }
  }

  .ui-button {
    width: 100% - $marginButton*2;
    margin: $marginButton;
    font-size: 90%;
  }
}

#content {
  margin-left: $sidebarWidth;
  margin-top: $headerHeight;
  padding: 20px;

  &.nosidebar {
    margin-left: 0px;
  }
  &.nopadding {
    padding: 0px;
  }

  #user_directory_listing {
    table {
      width: 100%;
      border-collapse: collapse;

      td.empty {
        width: 50%;
      }

      ul {
        padding: 15px;
        margin: 10px;
        min-height: 125px;
        position: relative;

        @include single-box-shadow;
        @include border-radius(5px);
        @include background-image(linear-gradient(#F3F3F3, #E6E6E6));

        li {
          padding: 3px;
          line-height: 1.1em;

          &.name {
            font-family: "Lato-Black";
            font-size: 120%;
          }
          &.email {
            font-family: "Lato-Light";
            font-size: 110%;

            a {
              text-decoration: none;
              color: inherit;
            }
          }
          &.photo {
            float: left;
            margin-right: 25px;

            img {
              height: 120px;
            }
          }
          &.staff_role {
            position: absolute;
            top: 10px;
            right: 15px;
            font-family: "Lato-Black";
            font-size: 175%;
            height: 32px;
            padding-right: 40px;
            background:{
              image: image-url("people/heart.png");
              repeat: no-repeat;
              position: right 3px;
            };
          }
        }
      }
    }
  }

  #gcal {
    width: 100%;
    //TODO make height exact
    height: 90%;
  }

  #event_create_fields, #event_view, #event_sign_ups {
    @include single-box-shadow;
    @include border-radius(10px);
    @include background-image(linear-gradient(#F3F3F3, #E6E6E6));
    padding: 20px 30px;
  }

  #event_create_fields {
    position: relative;
    padding-bottom: 60px;

    .field {
      margin-bottom: 3%;  

      label {
        font: {
          size: 120%;
          family: "Lato-Bold";
        };
      }

      input[type='text'], textarea {
        height: 32px;
        font-size: 14px;
        width: 50%;
        margin: 1%;
      }

      textarea {
        height: 100px;
      }
    }

    fieldset {
      padding: 20px;
      margin: 20px 0;
      width: 55%;
      position: relative;

      .field {
        input[type='text'] {
          width: 90%;
        }
      }

      .button_remove {
        $dim: 32px;
        position: absolute;
        right: 3px;
        top: 10px;
        width: $dim;
        height: $dim;
      }
    }

    input[type='submit'] {
      position: absolute;
      bottom: 10px;
      right: 10px;
    }
  }

  #event_list {
    .item {
      padding: 20px;
      margin-bottom: 30px;
      @include border-radius(10px);
      @include single-box-shadow;
      @include background-image(linear-gradient(#F3F3F3, #E6E6E6));

      h1 {
        font-family: "Lato-Black";
        font-size: 150%;
        margin-bottom: 15px;
      }

      .time {
        margin-bottom: 15px;
        font-size: 110%;
        font-family: "Lato-Bold";
      }
    }
  }
  
  #event_view {
    .title {
      font: {
        family: "Lato-Black";
        size: 300%;
      };
      margin-bottom: 30px;
    }
    .time {
      font: {
        family: "Lato-Bold";
        size: 125%;
      };
      margin-bottom: 30px;
    }
    .description {
      margin: 30px;
      font-size: 110%;
    }
    .fields {
      ul {
        margin: 10px 0;
      }
      .label {
        font-size: 120%;
        font-family: "Lato-Bold";
        margin: 25px 10px 15px 10px;
        padding: 5px 0;
        border-bottom: {
          style: solid;
          width: 1px;
          color: #c5c5c5;
        };
        width: 50%;
      }
      .value {
        font-size: 110%;
        margin: 5px 20px;
      }
    }
  }
  
  #event_sign_ups {
    table {
      width: 100%;
    }
    
    th, td {
      padding: 10px;
    }

    th.sortable {
      color: #444;
      cursor: pointer;
      text-decoration: underline;
    }
    th.sortable:hover {
      color: black;
    }
    th.sorted-asc, th.sorted-desc  {
      color: black;
    }

    td {
      background-color: white;
      color: #111;
    }
    td.odd {
      background-color: #666;
      color: white;
    }

    td.hovered {
      background-color: #598CCD;
      color: #000;
    }
  }
}

.event_create_sign_up_wizard {
  font-family: "Lato-Regular";

  .ui-dialog-title {
    font-family: "Lato-Bold";
  }

  #sign_up_form_create {
    float: left;
    width: 500px;
    height: 450px;
    overflow-y: scroll;

    fieldset {
      position: relative;
      margin-bottom: 10px;
      margin-right: 5px;
      padding: 0 10px;

      legend {
        font-size: 90%;
      }

      input[type='text'], textarea {
        width: 80%;
      }

      .label {
        font: {
          size: 80%;
          family: "Lato-Bold";
        };
        margin: 10px 5px 5px 5px;
      }

      .field {
        margin-top: 15px;
      }

      .button_remove {
        width: 22px;
        height: 22px;
        position: absolute;
        top: 10px;
        right: 5px;
      }
    }
  }

  .sign_up_form_builder_buttons_container {
    float: right;
    width: 150px;
    font: {
      size: 85%;
    };

    button {
      width: 100%;
    }
  }
}

.sign_up_form_render {
  form {
    height: 400px;
    overflow-y: scroll;
  }

  input[type='text'], textarea { 
    width: 80%;
  }

  fieldset {
    margin: 0px 10px 10px 5px;

    .text {
      font: {
        family: "Lato-Bold";
        size: 110%;
      };
      margin: 5px;
    }

    .help {
      font: {
        family: "Lato-Regular";
        size: 90%;
      };
      margin: 5px 10px 10px 10px;
    }
  }
}

#welcome {

  #logo {
    height: 37%;
    padding-top: 5%;
    text-align: center;
    @include background-image(
      linear-gradient(#990000, #3F0F10)
    );
    color: white;
    @include text-shadow(#000000 5px 5px 25px);
    font-size: 95px;
    font-variant:small-caps;
    font-family: "Freshman";
    overflow: hidden;
  }

  #photos {
    text-align: center;
    height: 60px;
    @include background-image(
      linear-gradient(#222222, #000000)
    );
    overflow: hidden;
    
    img {
      height: 100%;
      margin: 0 15px;
    }
  }

  #login {
    text-align: center;
    
    a {
      margin-top: 100px;
      font-size: 30px;
    }
  }
}

